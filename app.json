{
  "expo": {
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/logo.png",
          "color": "#C53030"
        }
      ],
      [
        "expo-background-fetch",
        {
          "minimumInterval": 15
        }
      ],
      [
        "expo-task-manager",
        {
          "backgroundPermissions": true
        }
      ],
         [
        "expo-task-manager",
        {
          "backgroundPermissions": true
        }
      ]
    ],
    "name": "Cosaif",
    "slug": "Cosaif",
    "owner": "vianko",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vianko.Cosaif",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "UIBackgroundModes": ["fetch", "remote-notification"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.vianko.Cosaif",
      "versionCode": 3,
      "permissions": ["RECEIVE_BOOT_COMPLETED"]
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "a95ca765-92da-4ccc-b85a-cbd8bfae4e94"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/a95ca765-92da-4ccc-b85a-cbd8bfae4e94",
      "enabled": true,
      "fallbackToCacheTimeout": 0
    }
  }
}
